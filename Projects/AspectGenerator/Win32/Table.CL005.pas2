procedure TCL005Item.ReadCmd(stream: TStream; vtrTemp: TVirtualStringTree;
  vCmd: PVirtualNode; CmdItem: TCmdItem);
var
  delta: integer;
  flds40: TLogicalData40;
  propindexCL005: TCL005Item.TPropertyIndex;
  vCmdProp: PVirtualNode;
  dataCmdProp: PAspRec;
begin
  delta := sizeof(TLogicalData128) - sizeof(TLogicalData40);
  stream.Read(flds40, sizeof(TLogicalData40));
  stream.Position := stream.Position + delta;
  New(self.PRecord);

  self.PRecord.setProp := TCL005Item.TSetProp(flds40);// тука се записва какво има като полета


  for propindexCL005 := Low(TCL005Item.TPropertyIndex) to High(TCL005Item.TPropertyIndex) do
  begin
    if not (propindexCL005 in self.PRecord.setProp) then
      continue;
    if vtrTemp <> nil then
    begin
      vCmdProp := vtrTemp.AddChild(vCmd, nil);
      dataCmdProp := vtrTemp.GetNodeData(vCmdProp);
      dataCmdProp.index := word(propindexCL005);
      dataCmdProp.vid := vvPregled;
    end;
    self.FillPropCL005(propindexCL005, stream);
  end;

  CmdItem.AdbItem := self;
end;

procedure TCL005Item.FillPropCL005(propindex: TPropertyIndex;
  stream: TStream);
var
  lenStr: Word;
begin
  case propindex of
    CL005_ADDRESS_ACT:
        begin
          stream.Read(lenStr, 2);
          SetLength(Self.PRecord.ADDRESS_ACT, lenStr);
          stream.Read(Self.PRecord.ADDRESS_ACT[1], lenStr);
        end;
            CL005_ADDRESS_DOGNZOK:
            begin
              stream.Read(lenStr, 2);
              SetLength(Self.PRecord.ADDRESS_DOGNZOK, lenStr);
              stream.Read(Self.PRecord.ADDRESS_DOGNZOK[1], lenStr);
            end;
            CL005_ADRES:
            begin
              stream.Read(lenStr, 2);
              SetLength(Self.PRecord.ADRES, lenStr);
              stream.Read(Self.PRecord.ADRES[1], lenStr);
            end;
            CL005_BANKA:
            begin
              stream.Read(lenStr, 2);
              SetLength(Self.PRecord.BANKA, lenStr);
              stream.Read(Self.PRecord.BANKA[1], lenStr);
            end;
            CL005_BANKOW_KOD:
            begin
              stream.Read(lenStr, 2);
              SetLength(Self.PRecord.BANKOW_KOD, lenStr);
              stream.Read(Self.PRecord.BANKOW_KOD[1], lenStr);
            end;
            CL005_BULSTAT:
            begin
              stream.Read(lenStr, 2);
              SetLength(Self.PRecord.BULSTAT, lenStr);
              stream.Read(Self.PRecord.BULSTAT[1], lenStr);
            end;
            CL005_COMPANYNAME:
            begin
              stream.Read(lenStr, 2);
