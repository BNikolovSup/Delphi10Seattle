    CL005_FNAME: ListForFinder[0].PRecord.FNAME := AText;
    CL005_FULLNAME: ListForFinder[0].PRecord.FULLNAME := AText;
    CL005_KOD_RAJON: ListForFinder[0].PRecord.KOD_RAJON := AText;
    CL005_KOD_RZOK: ListForFinder[0].PRecord.KOD_RZOK := AText;
