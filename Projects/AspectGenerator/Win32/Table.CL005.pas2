procedure TCL005Item.InsertCL005;
var
  CollType: TCollectionsType;
  metaPosition, dataPosition, PropPosition: cardinal;
  FPosMetaData, FLenMetaData, FPosData, FLenData: Cardinal;
  propIndx: TPropertyIndex;
  pCardinalData: ^Cardinal;
  pWordData: ^Word;
begin
  CollType := Aspects.Types.TCollectionsType.ctCL005;
  pCardinalData := pointer(PByte(buf));
  FPosMetaData := pCardinalData^;
  pCardinalData := pointer(PByte(buf) + 4);
  FLenMetaData := pCardinalData^;
  metaPosition :=  FPosMetaData + FLenMetaData;

  pCardinalData := pointer(PByte(buf) + 8);
  FPosData := pCardinalData^;
  pCardinalData := pointer(PByte(buf) + 12);
  FLenData := pCardinalData^;
  dataPosition :=  FPosData + FLenData;
  SaveAnyStreamCommand(@PRecord.setProp, SizeOf(PRecord.setProp), CollType, toInsert, FVersion, metaPosition + 4);
  case FVersion of
    0:
    begin
      pWordData := pointer(PByte(buf) + metaPosition);
      pWordData^  := word(CollType);
      pWordData := pointer(PByte(buf) + metaPosition + 2);
      pWordData^  := FVersion;
      inc(metaPosition, 4);
	  Self.DataPos := metaPosition;
	  
      for propIndx := Low(TPropertyIndex) to High(TPropertyIndex) do
      begin
        if Assigned(PRecord) and (propIndx in PRecord.setProp) then
        begin
          case propIndx of
            CL005_ADDRESS_ACT: SaveData(PRecord.ADDRESS_ACT, PropPosition, metaPosition, dataPosition);
            CL005_ADDRESS_DOGNZOK: SaveData(PRecord.ADDRESS_DOGNZOK, PropPosition, metaPosition, dataPosition);
            CL005_ADRES: SaveData(PRecord.ADRES, PropPosition, metaPosition, dataPosition);
            CL005_BANKA: SaveData(PRecord.BANKA, PropPosition, metaPosition, dataPosition);
            CL005_BANKOW_KOD: SaveData(PRecord.BANKOW_KOD, PropPosition, metaPosition, dataPosition);
            CL005_BULSTAT: SaveData(PRecord.BULSTAT, PropPosition, metaPosition, dataPosition);
            CL005_COMPANYNAME: SaveData(PRecord.COMPANYNAME, PropPosition, metaPosition, dataPosition);
            CL005_CONTRACT_DATE: SaveData(PRecord.CONTRACT_DATE, PropPosition, metaPosition, dataPosition);
            CL005_CONTRACT_RZOK: SaveData(PRecord.CONTRACT_RZOK, PropPosition, metaPosition, dataPosition);
            CL005_CONTRACT_TYPE: SaveData(PRecord.CONTRACT_TYPE, PropPosition, metaPosition, dataPosition);
            CL005_DAN_NOMER: SaveData(PRecord.DAN_NOMER, PropPosition, metaPosition, dataPosition);
            CL005_EGN: SaveData(PRecord.EGN, PropPosition, metaPosition, dataPosition);
            CL005_FNAME: SaveData(PRecord.FNAME, PropPosition, metaPosition, dataPosition);
            CL005_FULLNAME: SaveData(PRecord.FULLNAME, PropPosition, metaPosition, dataPosition);
            CL005_INVOICECOMPANY: SaveData(PRecord.INVOICECOMPANY, PropPosition, metaPosition, dataPosition);
            CL005_ISSUER_TYPE: SaveData(PRecord.ISSUER_TYPE, PropPosition, metaPosition, dataPosition);
            CL005_IS_SAMOOSIG: SaveData(PRecord.IS_SAMOOSIG, PropPosition, metaPosition, dataPosition);
            CL005_KOD_RAJON: SaveData(PRecord.KOD_RAJON, PropPosition, metaPosition, dataPosition);
            CL005_KOD_RZOK: SaveData(PRecord.KOD_RZOK, PropPosition, metaPosition, dataPosition);
            CL005_LNAME: SaveData(PRecord.LNAME, PropPosition, metaPosition, dataPosition);
            CL005_LNCH: SaveData(PRecord.LNCH, PropPosition, metaPosition, dataPosition);
            CL005_NAME: SaveData(PRecord.NAME, PropPosition, metaPosition, dataPosition);
            CL005_NAS_MQSTO: SaveData(PRecord.NAS_MQSTO, PropPosition, metaPosition, dataPosition);
            CL005_NEBL_USL: SaveData(PRecord.NEBL_USL, PropPosition, metaPosition, dataPosition);
            CL005_NOMER_LZ: SaveData(PRecord.NOMER_LZ, PropPosition, metaPosition, dataPosition);
            CL005_NOM_NAP: SaveData(PRecord.NOM_NAP, PropPosition, metaPosition, dataPosition);
            CL005_NZOK_NOMER: SaveData(PRecord.NZOK_NOMER, PropPosition, metaPosition, dataPosition);
            CL005_OBLAST: SaveData(PRecord.OBLAST, PropPosition, metaPosition, dataPosition);
            CL005_OBSHTINA: SaveData(PRecord.OBSHTINA, PropPosition, metaPosition, dataPosition);
            CL005_SELF_INSURED_DECLARATION: SaveData(PRecord.SELF_INSURED_DECLARATION, PropPosition, metaPosition, dataPosition);
            CL005_SMETKA: SaveData(PRecord.SMETKA, PropPosition, metaPosition, dataPosition);
            CL005_SNAME: SaveData(PRecord.SNAME, PropPosition, metaPosition, dataPosition);
            CL005_UPRAVITEL: SaveData(PRecord.UPRAVITEL, PropPosition, metaPosition, dataPosition);
            CL005_VIDFIRMA: SaveData(PRecord.VIDFIRMA, PropPosition, metaPosition, dataPosition);
            CL005_VID_IDENT: SaveData(PRecord.VID_IDENT, PropPosition, metaPosition, dataPosition);
            CL005_VID_PRAKTIKA: SaveData(PRecord.VID_PRAKTIKA, PropPosition, metaPosition, dataPosition);
          end;
        end
        else
        begin
          SaveNull(metaPosition);
        end;
      end;
      pCardinalData := pointer(PByte(buf) + 4);
      pCardinalData^  := metaPosition - FPosMetaData;
      pCardinalData := pointer(PByte(buf) + 12);
      pCardinalData^  := dataPosition - FPosData;
    end;
  end;
end;

