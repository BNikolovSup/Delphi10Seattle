procedure T!TableName!Coll.SetCell(Sender: TObject; const AColumn: TColumn; const ARow: Integer; var AValue: String);
var
  isOld: Boolean;
  !TableName!: T!TableName!Item;
  ACol: Integer;
begin
  if Count = 0 then Exit;
  ACol := TVirtualModeData(Sender).IndexOf(AColumn);
  isOld := False;
  !TableName! := Items[ARow];
  if not Assigned(!TableName!.PRecord) then
  begin
    New(!TableName!.PRecord);
    !TableName!.PRecord.setProp := [];
	CntUpdates := CntUpdates + 1;
  end
  else
  begin
    case T!TableName!Item.TPropertyIndex(ACol) of
      !ProcCompareOldCell!
    end;
  end;
  if isOld then
  begin
    Exclude(!TableName!.PRecord.setProp, T!TableName!Item.TPropertyIndex(ACol));
    if !TableName!.PRecord.setProp = [] then
    begin
      Dispose(!TableName!.PRecord);
      !TableName!.PRecord := nil;
      CntUpdates := CntUpdates - 1;
      Exit;
    end;
  end;
  Include(!TableName!.PRecord.setProp, T!TableName!Item.TPropertyIndex(ACol));
  case T!TableName!Item.TPropertyIndex(ACol) of
    !ProcSetCell!
  end;
end;

