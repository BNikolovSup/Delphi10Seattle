procedure T!TableName!Coll.GetFieldText(Sender: TObject; const ACol, ARow: Integer; var AFieldText: String);
var
  !TableName!: T!TableName!Item;
  prop: T!TableName!Item.TPropertyIndex;
begin
  if Count = 0 then Exit;

  !TableName! := Items[ARow];
  prop := T!TableName!Item.TPropertyIndex(ACol);
  if Assigned(!TableName!.PRecord) and (prop in !TableName!.PRecord.setProp) then
  begin
    GetCellFromRecord(ACol, !TableName!, AFieldText);
